<AnchorPane prefHeight="621.0" prefWidth="391.0" style="-fx-background-radius: 30;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.controller.SettingPaneController">
   <children>
      <Pane layoutY="537.0" prefHeight="621.0" prefWidth="519.0" style="-fx-background-color: #9CAAAA;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <Button fx:id="recoverButton" layoutX="305.0" layoutY="577.0" mnemonicParsing="false" onAction="#recover" styleClass="button2" stylesheets="@css/buttonattend.css" text="恢复默认值" />
            <Button fx:id="saveSettingButton" layoutX="436.0" layoutY="577.0" mnemonicParsing="false" onAction="#saveSetting" prefHeight="30.0" prefWidth="59.0" styleClass="button2" stylesheets="@css/buttonattend.css" text="保存" />
            <Button fx:id="closeButton" layoutX="24.0" layoutY="577.0" mnemonicParsing="false" onAction="#getmainPane" styleClass="button2" stylesheets="@css/buttonattend.css" text="返回" />
         </children>
      </Pane>
      <TabPane layoutY="-2.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="345.0" prefWidth="519.0" stylesheets="@css/settingtab.css" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab text="视频设置">
               <content>
                  <Pane fx:id="videoPane" prefHeight="396.0" prefWidth="485.0" style="-fx-background-color: #9CAAAA;">
                     <children>
                        <Label layoutX="21.0" layoutY="48.0" text="视频保存地址：" />
                        <TextField fx:id="fileText" layoutX="129.0" layoutY="43.0" prefHeight="30.0" prefWidth="273.0" style="-fx-background-radius: 30;" stylesheets="@css/buttonattend.css" />
                        <Button fx:id="showFileButton" layoutX="439.0" layoutY="42.0" mnemonicParsing="false" onAction="#showFile" prefHeight="30.0" prefWidth="59.0" styleClass="button2" stylesheets="@css/buttonattend.css" text="浏览" />
                        <Label layoutX="60.0" layoutY="263.0" text="分辨率：" />
                        <ChoiceBox fx:id="formatChoiceBox" layoutX="129.0" layoutY="119.0" prefWidth="150.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                        <Label layoutX="45.0" layoutY="124.0" text="视频格式：" />
                        <Label layoutX="45.0" layoutY="194.0" text="视频帧率：" />
                        <ChoiceBox fx:id="frameChoiceBox" layoutX="129.0" layoutY="189.0" prefWidth="150.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                        <ChoiceBox fx:id="resolutionChoiceBox" layoutX="129.0" layoutY="258.0" prefWidth="150.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                     </children>
                  </Pane>
               </content>
            </Tab>
            <Tab text="音频设置">
               <content>
                  <Pane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #9CAAAA;">
                     <children>
                     <Label layoutX="86.0" layoutY="165.0" text="比特率：" />
                     <Label layoutX="71.0" layoutY="45.0" text="音频质量：" />
                     <ChoiceBox fx:id="audioQualityBox" layoutX="164.0" layoutY="40.0" prefHeight="30.0" prefWidth="165.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                     <Label layoutX="86.0" layoutY="100.0" text="采样率：" />
                     <ChoiceBox fx:id="sampleRateBox" layoutX="164.0" layoutY="96.0" prefHeight="30.0" prefWidth="165.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                     <ChoiceBox fx:id="audioBitrateBox" layoutX="164.0" layoutY="160.0" prefHeight="30.0" prefWidth="165.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                     </children>
                  </Pane>
               </content>
            </Tab>
            <Tab text="截图设置">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Pane layoutY="-1.0" prefHeight="542.0" prefWidth="519.0" style="-fx-background-color: #9CAAAA;">
                           <children>
                              <Label layoutX="46.0" layoutY="89.0" text="图片保存格式：" />
                              <ComboBox fx:id="audioQuality3" layoutX="167.0" layoutY="84.0" prefHeight="30.0" prefWidth="165.0" styleClass="combobox1" stylesheets="@css/buttonattend.css" />
                           </children>
                        </Pane>
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>




